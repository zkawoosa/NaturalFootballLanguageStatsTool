{
  "name": "nfl-query",
  "version": "0.1.0",
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10 <11"
  },
  "private": true,
  "type": "module",
  "packageManager": "npm@10.9.0",
  "scripts": {
    "test": "node --test --experimental-strip-types \"src/**/*.test.ts\"",
    "test:quiet": "NFL_QUERY_TEST_QUIET_LOGS=1 node --test --experimental-strip-types \"src/**/*.test.ts\"",
    "dev": "node --test --watch --experimental-strip-types \"src/**/*.test.ts\"",
    "build": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --check \"src/**/*.ts\" \"*.md\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"*.md\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.4.2"
  }
}
